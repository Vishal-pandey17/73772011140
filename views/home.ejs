<html>
    <head>
        <title>
            <%= title %>
        </title>
    </head>
    <body>
        <h1>Sign In</h1>
        
        <form id="new-contact-form" action="/train/register" method="POST">
            <input type="text" name="company-name" placeholder="Company Name" required>
            <input type="text" name="owner-name" placeholder="owner Name" required>
            <input type="number" name="Roll-No" placeholder="Roll No" required>
            <input type="email" name="email" placeholder="Email" required>
            <input type="text" name="accessCode" placeholder="Access Code" required>
            <button type="submit">ADD</button>
          </form>
  
    </body>
</html>
